{
    "api": {
      "services": [
        "HandlerService",
        "StatsService"
      ],
      "tag": "api"
    },
    "stats": {
    },
    "inbound": {
      "port": 443,
      "listen":"0.0.0.0",
      "protocol": "vmess",
      "settings": {
        "clients": []
      },
      "streamSettings":{
          "network":"h2",
          "httpSettings":{
              "host": ["hk-03.ali-ip.us"],
              "path":"/pic/download"
          },
          "security":"tls",
          "tlsSettings":{
          "certificates":[
                  {
                      "certificateFile":"/etc/v2ray/v2ray.crt",
                      "keyFile":"/etc/v2ray/v2ray.key"
                  }
              ]
          }
      },
      "tag": "proxy"
    },
    "inboundDetour": [{
      "listen": "127.0.0.1",
      "port": 8301,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "0.0.0.0"
      },
      "tag": "api"
    }],
    "log": {
      "loglevel": "none",
      "access": "/var/log/v2ray/access.log",
      "error": "/var/log/v2ray/error.log"
    },
    "outbound": {
      "protocol": "freedom",
      "settings": {}
    },
    "routing": {
      "settings": {
        "rules": [{
          "inboundTag": [
            "api"
          ],
          "outboundTag": "api",
          "type": "field"
        }]
      },
      "strategy": "rules"
    },
    "policy": {
      "levels": {
        "1": {
          "handshake": 4,
          "connIdle": 300,
          "uplinkOnly": 5,
          "downlinkOnly": 30,
          "statsUserUplink": true,
          "statsUserDownlink": true
        }
      }
    }
  }